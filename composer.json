{
    "name": "tystr/aws-bundle",
    "description": "This bundle integrates the AWS SDK for PHP into symfony2",
    "homepage": "http://github.com/tystr/TystrAwsBundle",
    "support": {
        "issues": "https://github.com/tystr/TystrAwsBundle/issues"
    },
    "keywords": ["aws", "amazon web services", "s3", "ec2", "route53", "cloudfront", "dynamodb", "emr", "elastic beanstalk", "opsworks", "bundle", "symfony2", "symfony"],
    "type": "symfony2-bundle",
    "require": {
        "php": ">=5.3.3",
        "aws/aws-sdk-php": "~2.0",
        "symfony/framework-bundle": "~2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Stroud",
            "email": "tyler@tylerstroud.com"
        }
    ],
    "autoload": {
        "psr-0": {"Tystr\\Bundle\\AwsBundle": ""}
    },
    "target-dir": "Tystr/Bundle/AwsBundle",
    "suggest": {
        "symfony/finder": "Easy way to traverse an S3 bucket"
    }
}
